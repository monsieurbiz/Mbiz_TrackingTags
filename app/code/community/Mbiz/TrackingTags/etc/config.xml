<config>
    <modules>
        <Mbiz_TrackingTags>
            <version>0.1.0</version>
        </Mbiz_TrackingTags>
    </modules>
    <global>
        <helpers>
            <mbiz_trackingtags>
                <class>Mbiz_TrackingTags_Helper</class>
            </mbiz_trackingtags>
        </helpers>
        <models>
            <mbiz_trackingtags>
                <class>Mbiz_TrackingTags_Model</class>
            </mbiz_trackingtags>
        </models>
        <blocks>
            <mbiz_trackingtags>
                <class>Mbiz_TrackingTags_Block</class>
            </mbiz_trackingtags>
        </blocks>
    </global>
    <frontend>
        <layout>
            <updates>
                <mbiz_trackingtags module="Mbiz_TrackingTags">
                    <file>mbiz_trackingtags.xml</file>
                </mbiz_trackingtags>
            </updates>
        </layout>
    </frontend>
    <default>
        <design>
            <mbiz_trackingtags>
                <homepage_active>0</homepage_active>
                <homepage_tag/>
                <product_active>0</product_active>
                <product_tag/>
                <category_active>0</category_active>
                <category_tag/>
                <cart_active>0</cart_active>
                <cart_tag/>
                <checkout_success_active>0</checkout_success_active>
                <checkout_success_tag/>
            </mbiz_trackingtags>
        </design>
    </default>
</config>